<view><uni-nav-bar vue-id="1" title="地址栏" statusBar="{{false}}" shadow="{{false}}" fixed="{{true}}" data-event-opts="{{[['^clickRight',[['goEdit']]]]}}" onClickRight="__e" onVueInit="__l"><view slot="right"><text class="iconfont icon-jia"></text></view></uni-nav-bar><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><block><uni-swipe-action vue-id="{{'2-'+index}}" options="{{options}}" data-event-opts="{{[['^click',[['bindClick',['$event',index]]]]]}}" onClick="__e" onVueInit="__l"><uni-list-item onClick="__e" vue-id="{{('3-'+index)+','+('2-'+index)}}" data-event-opts="{{[['^click',[['choose',['$0'],[[['list','',index]]]]]]]}}" onVueInit="__l"><view class="text-secondary"><view class="d-flex a-center"><text class="main-text-color">{{item.name}}</text>{{''+item.phone+''}}<block a:if="{{item.isdefault}}"><text class="main-text-color">[默认]</text></block></view><view>{{item.path}}</view><view>{{item.detailPath}}</view></view></uni-list-item></uni-swipe-action></block></block></view>