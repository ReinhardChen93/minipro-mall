<view class="d-flex flex-column" style="height:100%;"><view class="d-flex bg-white a-center font-md text-muted border-top border-bottom border-light-secondary"><block a:for="{{tabBars}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" class="{{(('flex-1 d-flex a-center j-center py-2')+' '+(tabIndex===index?'tabactive':''))}}" onTap="__e">{{''+item.name+''}}</view></block></view><scroll-view class="flex-1" style="background:#F5F5F5;" scroll-y><block a:for="{{tabBars}}" a:for-item="tab" a:for-index="tabI" a:key="tabI"><block><view hidden="{{!(tabI===tabIndex)}}" class="position-relative" style="min-height:440rpx;"><block a:if="{{tab.list.length>0}}"><view class="p-3"><block a:for="{{tab.list}}" a:for-item="item" a:for-index="index" a:key="index"><coupon vue-id="{{'1-'+tabI+'-'+index}}" item="{{item}}" index="{{index}}" data-event-opts="{{[['^click',[['chooseCoupon',['$0'],[[['tabBars','',tabI],['list','',index]]]]]]]}}" onClick="__e" onVueInit="__l"><block a:if="{{item.disabled}}"><text>已使用</text></block><block a:else><text>{{item.status?'去使用':validText}}</text></block></coupon></block></view></block><block a:else><no-thing vue-id="{{'2-'+tabI}}" icon="{{tab.no_thing}}" msg="{{tab.msg}}" onVueInit="__l"></no-thing></block></view></block></block></scroll-view></view>