<view><view class="d-flex a-center" style="height:90rpx;"><view class="d-flex a-center j-center" style="width:85rpx;"><text class="iconfont icon-xiaoxi"></text></view><view data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="flex-1 bg-light rounded d-flex a-center text-light-muted" style="height:65rpx;" onTap="__e"><text class="iconfont icon-sousuo mx-2"></text>智能积木</view><view class="d-flex a-center j-center" style="width:85rpx;"><text class="iconfont icon-richscan_icon"></text></view></view><scroll-view class="border-bottom scroll-row" style="height:80rpx;" scroll-x scroll-into-view="{{scrollinto}}" scroll-with-animation="{{true}}"><block a:for="{{tabBars}}" a:for-item="item" a:for-index="index" a:key="index"><view class="scroll-row-item px-3" style="height:80rpx;line-height:80rpx;" id="{{'tab'+index}}" data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" onTap="__e"><text class="{{(('font-md')+' '+(tabIndex===index?'main-text-color border-bottom input-border-color':''))}}">{{item.name}}</text></view></block></scroll-view><swiper style="{{('height:'+scrollH+'px')}}" duration="{{150}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['onChangeTab',['$event']]]]]}}" onChange="__e"><block a:for="{{newsitems}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item><scroll-view style="{{('height:'+scrollH+'px')}}" scroll-y="true" data-event-opts="{{[['scrollToLower',[['loadmore',[index]]]]]}}" onScrollToLower="__e"><view class="swiper-item"><block a:for="{{item.list}}" a:for-item="list" a:for-index="listIndex" a:key="listIndex"><block><block a:if="{{list.type==='swiper'}}"><swiper-image vue-id="{{'1-'+index+'-'+listIndex}}" resdata="{{list.data}}" onVueInit="__l"></swiper-image></block><block a:if="{{list.type==='indexnavs'}}"><index-nav vue-id="{{'2-'+index+'-'+listIndex}}" resdata="{{list.data}}" onVueInit="__l"></index-nav><divider vue-id="{{'3-'+index+'-'+listIndex}}" onVueInit="__l"></divider></block><block a:if="{{list.type==='threeAdv'}}"><three-adv vue-id="{{'4-'+index+'-'+listIndex}}" resdata="{{list.data}}" onVueInit="__l"></three-adv><divider vue-id="{{'5-'+index+'-'+listIndex}}" onVueInit="__l"></divider></block><block a:else><block a:if="{{list.type==='oneAdv'}}"><card vue-id="{{'6-'+index+'-'+listIndex}}" headTitle="{{list.data.title}}" bodyCover="{{list.data.cover}}" onVueInit="__l"></card></block><block a:else><block a:if="{{list.type==='commonList'}}"><view class="row j-sb"><block a:for="{{list.data}}" a:for-item="item2" a:for-index="index2" a:key="index2"><block><common-list vue-id="{{'7-'+index+'-'+listIndex+'-'+index2}}" item="{{item2}}" index="{{index2}}" onVueInit="__l"></common-list></block></block></view></block></block></block></block></block></view><view class="d-flex j-center text-light-muted font-md py-3">{{''+item.loadtext+''}}</view></scroll-view></swiper-item></block></swiper></view>