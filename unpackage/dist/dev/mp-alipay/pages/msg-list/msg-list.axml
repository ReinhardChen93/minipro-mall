<view class="p-2"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view class="mb-2 p-2 bg-white rounded" hover-class="bg-light" data-event-opts="{{[['tap',[['open',['$0'],[[['list','',index]]]]]]]}}" onTap="__e"><view class="text-dark font-md">{{item.title}}</view><view class="font text-light-muted">{{''+item.desc+''}}</view><view class="font text-light-muted">{{item.create_time}}</view></view></block></view>